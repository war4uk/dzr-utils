{
	"version": "0.1.0",
	"command": "gulp",
	"isShellCommand": true,	
	"args": [
		"--gulpfile",
				"swarm/gulpfile.js"
	],
	"tasks": [
		{
			"args": [
				
			],
			"taskName": "build",
			// Make this the default build command.
			"isBuildCommand": true,
			// Show the output window only if unrecognized errors occur.
			"showOutput": "silent",			
			// Use the standard less compilation problem matcher.
			"problemMatcher": [
				"$tsc"
			]
		}		
	]
}